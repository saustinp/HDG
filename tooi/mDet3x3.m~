function d = mDet3x3(A,i,j)

d = A(i,1,j,1) .* A(i,2,j,2) - A(i,1,j,2) .* A(i,2,j,1);


% detJ = Jg(:,:,1,1).*Jg(:,:,2,2) - Jg(:,:,1,2).*Jg(:,:,2,1);

d = A(i,1,j,1).*A(i,2,j,2).*A(i,3,j,3) - A(i,1,j,1).*A(i,3,j,2).*A(i,2,j,3)+ ...
    A(:,:,2,1).*Jg(:,:,3,2).*Jg(:,:,1,3) - Jg(:,:,2,1).*Jg(:,:,1,2).*Jg(:,:,3,3)+ ...
      Jg(:,:,3,1).*Jg(:,:,1,2).*Jg(:,:,2,3) - Jg(:,:,3,1).*Jg(:,:,2,2).*Jg(:,:,1,3);            

% detJ = Jg(:,:,1,1).*Jg(:,:,2,2).*Jg(:,:,3,3) - Jg(:,:,1,1).*Jg(:,:,3,2).*Jg(:,:,2,3)+ ...
%       Jg(:,:,2,1).*Jg(:,:,3,2).*Jg(:,:,1,3) - Jg(:,:,2,1).*Jg(:,:,1,2).*Jg(:,:,3,3)+ ...
%       Jg(:,:,3,1).*Jg(:,:,1,2).*Jg(:,:,2,3) - Jg(:,:,3,1).*Jg(:,:,2,2).*Jg(:,:,1,3);            
